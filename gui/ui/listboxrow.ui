<?xml version="1.0" encoding="UTF-8"?>
<interface domain="tasklist">
    <object id="tasklistBoxRow" class="GtkListBoxRow">
        <property name="hexpand">1</property>
        <child>
            <object id="contentBox" class="GtkBox">
                <property name="orientation">horizontal</property>
                <child>
                    <object id="leftSideContent" class="GtkBox">
                        <property name="hexpand">1</property>
                        <property name="orientation">vertical</property>
                        <child>
                            <object id="taskName" class="GtkLabel">
                            </object>
                        </child>
                        <child>
                            <object id="sep0" class="GtkSeparator">
                            </object>
                        </child>
                        <child>
                            <object id="taskDescription" class="GtkTextView">
                                    <property name="visible">True</property>
                                    <property name="editable">False</property>
                                    <property name="wrap-mode">word</property>
                                    <property name="height-request">50</property>
                                    <property name="vexpand">1</property>
                            </object>
                        </child>
                    </object>
                </child>
                <child>
                    <object id="expander" class="GtkBox">
                        <property name="hexpand">1</property>
                    </object>
                </child>
                <child>
                    <object id="rightSideContent" class="GtkBox">
                        <property name="orientation">horizontal</property>
                        <child>
                            <object id="taskDeadline" class="GtkLabel">
                            </object>
                        </child>
                        <child>
                            <object id="propertiesButton" class="GtkButton">
                                <property name="vexpand">1</property>
                            </object>
                        </child>
                    </object>
                </child>
            </object>
        </child>
    </object>
</interface>